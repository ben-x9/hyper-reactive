{
  "name": "react-tooling",
  "version": "0.27.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Tooling for React",
  "repository": "github.com/ben-x9/react-tooling",
  "authors": [
    "Ben Lambert",
    "Jalal Chaabane"
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "typescript"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepare": "npm run build",
    "compile": "tsc",
    "format": "prettier --write '**/*.{ts*(x),js*(on),scss}' '!dist/**'",
    "check:format": "prettier -l '**/*.{ts*(x),js*(on),scss}' '!dist/**'",
    "check": "npm run compile && npm run check:format"
  },
  "dependencies": {
    "functools-ts": "^0.1.0",
    "history": "^4.9.0",
    "react": "^16.8.6",
    "react-hot-loader": "^4.8.7",
    "react-redux": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "hydra-dispatch": "^0.1.0",
    "hydra-dispatch-redux": "^0.1.0",
    "redux": "^4.0.1",
    "remote-redux-devtools": "^0.5.16",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/node": "^8.0.54",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.0",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^6.0.7",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/remote-redux-devtools": "^0.5.2",
    "prettier": "^1.16.3",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^3.2.4"
  }
}
